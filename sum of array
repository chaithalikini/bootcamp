#include <stdio.h>
int sum(int arr[], int n) {
  int sum = 0;
  for (int i = 0; i < n; i++) {
    sum += arr[i];
  }
  return sum;
}
void main() {
  int n;
  printf("Enter the size of the array: ");
  scanf("%d", &n);
  int arr[size];
  printf("Enter the elements of the array:\n");
  for (int i = 0; i < n; i++) {
    scanf("%d", &arr[i]);
  }
  int totalSum = sum(arr, n);
  printf("Sum of the array elements: %d\n", totalSum);
}

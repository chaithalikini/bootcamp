#include <stdio.h>
void PrefixSum(int sales[],int prefixSum[],int n) {
    prefixSum[0]=sales[0];
    for (int i=1;i<n;i++)
 {
        prefixSum[i]=prefixSum[i-1]+sales[i];
    }
}
int main() {
    int sales[]={10,20,15,30,25};
    int n=sizeof(sales)/sizeof(sales[0]);
    int prefixSum[n];
    PrefixSum(sales,prefixSum,n);
    printf("PrefixSum[]=");
    for (int i=0;i<n;i++) {
        printf("%d ",prefixSum[i]);
    }
    printf("\n");
    return 0;
}
